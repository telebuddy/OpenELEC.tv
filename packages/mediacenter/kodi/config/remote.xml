<keymap>
  <global>
    <remote>
      <three>VolumeUp</three>
      <six>VolumeDown</six>
      <nine>Mute</nine>
      <play>Play</play>
      <pause>Pause</pause>
      <stop>Stop</stop>
      <forward>FastForward</forward>
      <reverse>Rewind</reverse>
      <left>Left</left>
      <right>Right</right>
      <up>Up</up>
      <down>Down</down>
      <select>Select</select>
      <pageplus>PageUp</pageplus>
      <pageminus>PageDown</pageminus>
      <back>ParentDir</back>
      <menu>PreviousMenu</menu>
      <title>ContextMenu</title>
      <info>Info</info>
      <skipplus>SkipNext</skipplus>
      <skipminus>SkipPrevious</skipminus>
      <display>FullScreen</display>
      <start>XBMC.ActivateWindow(Home)</start>
      <record>Screenshot</record>
      <volumeplus>VolumeUp</volumeplus>
      <volumeminus>VolumeDown</volumeminus>
      <mute>Mute</mute>
      <!-- <dvd>XBMC.ActivateWindow(PlayerControls)</dvd>
      <myvideo>XBMC.ActivateWindow(MyVideos)</myvideo> -->
      <myvideo>XBMC.ActivateWindow(MyVideoLibrary,MovieTitles,return)</myvideo>
      <mymusic>XBMC.ActivateWindow(MusicLibrary,Albums,return)</mymusic>
      <mytv>XBMC.ActivateWindow(MyVideoLibrary,tvshowtitles,return)</mytv>
      <mypictures>XBMC.ActivateWindow(MyPictures)</mypictures>
      <hash>XBMC.ActivateWindow(Settings)</hash>
      <star>XBMC.EjectTray()</star>
      <xbox>XBMC.ActivateWindow(shutdownmenu)</xbox>
      <power>XBMC.Powerdown()</power>
    </remote>
  </global>

  <Home>
    <remote>
      <menu>XBMC.Skin.ToggleSetting(HomeViewToggle)</menu>
      <info>XBMC.ActivateWindow(SystemInfo)</info>
      <clear>XBMC.ActivateWindow(Weather)</clear>
      <hash>XBMC.ActivateWindow(Settings)</hash>
    </remote>
  </Home>

  
  <MyFiles>
    <remote>
      <zero>Delete</zero>
      <one>Highlight</one>
      <four>Copy</four>
      <seven>Move</seven>
      <clear>Delete</clear>
      <three>VolumeUp</three>
      <six>VolumeDown</six>
      <nine>Mute</nine>
    </remote>
  </MyFiles>

  <MyMusicPlaylist>
    <remote>
      <back>Playlist</back> <!-- Close playlist -->
      <clear>Delete</clear>
      <zero>Delete</zero>
      <two>MoveItemUp</two>
      <five>MoveItemDown</five>
      <three>VolumeUp</three>
      <six>VolumeDown</six>
      <nine>Mute</nine>
    </remote>
  </MyMusicPlaylist>

  <MyMusicPlaylistEditor>
    <remote>
      <zero>Queue</zero>
    </remote>
  </MyMusicPlaylistEditor>

  
  <MyMusicFiles>
     <remote>
      <zero>Queue</zero>
      <star>Home</star>
      <three>VolumeUp</three>
      <six>VolumeDown</six>
      <menu>XBMC.ActivateWindow(MusicOSD)</menu>
      <nine>Mute</nine>
    </remote>
  </MyMusicFiles>

  <MyMusicLibrary>
    <remote>
      <zero>Queue</zero>
      <star>Home</star>
      <three>VolumeUp</three>
      <six>VolumeDown</six>
      <menu>XBMC.ActivateWindow(MusicOSD)</menu>
      <nine>Mute</nine>
    </remote>
  </MyMusicLibrary>

  <FullscreenVideo>
    <remote>
      <left>StepBack</left>
      <right>StepForward</right>
      <up>BigStepForward</up>
      <down>BigStepBack</down>
      <back>SmallStepBack</back>
      <menu>OSD</menu>
      <!-- <start>OSD</start> -->
      <title>CodecInfo</title>
      <!-- <enter>AspectRatio</enter> -->
      <info>Info</info>
      <display>AspectRatio</display>
   </remote>
  </FullscreenVideo>

  <FullscreenInfo>
    <remote>
      <title>CodecInfo</title>
      <info>Close</info>
      <menu>OSD</menu>
    </remote>
  </FullscreenInfo>

  <PlayerControls>
    <remote>
      <menu>Close</menu>
      <skipplus>SkipNext</skipplus>
      <skipminus>SkipPrevious</skipminus>
    </remote>
  </PlayerControls>

  <Visualisation>
    <remote>
      <left>PreviousPreset</left>
      <right>NextPreset</right>
      <up>IncreaseRating</up>
      <down>DecreaseRating</down>
      <back>LockPreset</back>
      <title>CodecInfo</title>
      <!-- <start>XBMC.ActivateWindow(MusicOSD)</start> -->
      <start>XBMC.ActivateWindow(Home)</start>
      <select>XBMC.ActivateWindow(VisualisationPresetList)</select>
      <menu>XBMC.ActivateWindow(MusicOSD)</menu>
      <info>Info</info>
    </remote>
  </Visualisation>

  <MusicOSD>
    <remote>
      <menu>Close</menu>
      <title>Info</title>
      <info>CodecInfo</info>
    </remote>
  </MusicOSD>

  <VisualisationSettings>
    <remote>
      <menu>Close</menu>
    </remote>
  </VisualisationSettings>

  <VisualisationPresetList>
    <remote>
      <menu>Close</menu>
    </remote>
  </VisualisationPresetList>

  <SlideShow>
    <remote>
      <zero>ZoomNormal</zero>
      <one>ZoomLevel1</one>
      <two>ZoomLevel2</two>
      <three>ZoomLevel3</three>
      <four>ZoomLevel4</four>
      <five>ZoomLevel5</five>
      <six>ZoomLevel6</six>
      <seven>ZoomLevel7</seven>
      <eight>ZoomLevel8</eight>
      <nine>ZoomLevel9</nine>
      <info>CodecInfo</info>
      <skipplus>NextPicture</skipplus>
      <skipminus>PreviousPicture</skipminus>
      <title>Info</title>
      <select>Rotate</select>
      <back>PreviousMenu</back>
    </remote>
  </SlideShow>

  <ScreenCalibration>
   <remote>
      <select>NextCalibration</select>
      <enter>NextCalibration</enter>
      <zero>ResetCalibration</zero>
      <display>NextResolution</display>
